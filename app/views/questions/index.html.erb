<p>
 <h1><%= "#{@survey.title} for #{@survey.group}" %> </h1>

  <%= link_to "Edit Survey Name and Group", edit_survey_path(@survey) %>

</p>
<p><%= link_to "New Question", new_survey_question_path(@survey, @question) %></p>

  <% @questions.each do |question| %>
  <tr>
    <td><%= link_to "#{question.body}", survey_question_path(@survey, question) %></td>

    <p>
      <%= link_to "Edit Question", edit_survey_question_path(@survey, question) %>
    </p>
    <p>
      <%= link_to "Delete Question", survey_question_path(@survey, question), method: :delete %>
    </p>
  </tr>

  <% end %>
